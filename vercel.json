{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.4"
    },
    {
      "src": "public/index.php",
      "use": "vercel-php@0.7.4"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ]
}
